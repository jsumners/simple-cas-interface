{
  "name": "simple-cas-interface",
  "version": "1.0.2",
  "description": "Provides an interface for authenticating and validating with a JASIG CAS server",
  "main": "cas.js",
  "scripts": {
    "test": "tap --no-cov 'test/**/*.test.js'",
    "lint": "standard cas.js lib/*.js test/*.js",
    "doc": "jsdoc2md cas.js lib/*.js > api.md"
  },
  "precommit": [
    "lint",
    "test"
  ],
  "keywords": [
    "cas",
    "jasig",
    "apereo"
  ],
  "author": "James Sumners",
  "license": "MIT",
  "bugs": "https://github.com/jsumners/simple-cas-interface/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/jsumners/simple-cas-interface.git"
  },
  "dependencies": {
    "abstract-logging": "^1.0.0",
    "joi": "^10.2.2",
    "request": "^2.69.0",
    "xml2js": "^0.5.0"
  },
  "devDependencies": {
    "jsdoc-to-markdown": "^3.0.0",
    "pre-commit": "^1.2.2",
    "standard": "^8.6.0",
    "tap": "^10.1.1"
  }
}
